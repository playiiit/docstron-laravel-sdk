{
    "name": "docstron/laravel-sdk",
    "description": "Laravel SDK for Docstron API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Docstron",
            "email": "support@docstron.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Docstron\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docstron\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Docstron\\Laravel\\DocstronServiceProvider"
            ],
            "aliases": {
                "Docstron": "Docstron\\Laravel\\Facades\\Docstron"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}